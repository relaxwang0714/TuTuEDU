 @import"../../templates/nav.wxss"; 
page{  
  display: flex;  
  flex-direction: column;   
  width: 100%;
  box-sizing: border-box;
  padding-top:33rpx; 
}  
.search_bar{
  display: flex;
  justify-content: center;
  width: 100%;
  margin-bottom: 20rpx;
}
.search_bar .search{
  width: 90%;
  height: 70rpx;
  border-radius: 80rpx;
  border:2px solid #909090;
}
.mainBar{
  display: flex;
  flex-direction: row;
}
.mainBar .mainTitle{
  width: 20%;
  font-size: 32rpx;
  text-align: center;
}
.mainBar .mainTitle.active{
  width: 20%;
  font-size: 32rpx;
  text-align: center;
  color: #db6f09;
}
.navBar{
  display: flex;
  flex-direction: row;
  box-shadow:16rpx 16rpx 72rpx #f0f0f0;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  padding: 10rpx 0;
}
.navBar .navtitle{
  width: 16%;
  font-size: 27rpx;
  text-align: center;
  margin-left: 10rpx;
}
.navBar .navtitle.active{
  width: 16%;
  font-size: 27rpx;
  text-align: center;
  color: #db6f09;
}