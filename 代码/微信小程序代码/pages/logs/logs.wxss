/* pages/logs/logs.wxss */
@import"../../templates/nav.wxss"; 
page{  
  display: flex;  
  flex-direction: column;  
  height: 100%;  
  width: 100%;
  box-sizing: border-box;
  padding-top:33rpx; 
  padding-right: 50rpx;
  padding-left: 50rpx;
}  
.navBar{
    height: 80rpx;
    width: 100%;
    display: flex;
    flex-direction: row;
}
.navItem{
  margin-right: 30rpx;
  width: 30%;
  height: 60rpx;
  text-align: center;
  box-shadow: 3rpx 3rpx 12rpx #f0f0f0;
  border-radius: 170rpx;
  font-size: 28rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.navItem.active{
  color: white;
  background-color: #eeb478;
}

