.table_top{
  width: 100%;
  height: 250rpx;
  background-color: #eeb478;
  display: flex;
  justify-content: center;
  align-items: center;
  color: white
}
.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
    display: block;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #f8f1ec;
}
.th {
  width: 40%;
  justify-content: center;
  background: #f0f0f0;
  color:gray;
  display: flex;
  height: 3rem;
  align-items: center;
}